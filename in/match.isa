module Option

    type Option a = None | Some a;

module Main
    
    val foo: Option::Option () -> int;
    let foo a =
        match a with
            Option::Some () -> 10,
            Option::None -> 10,
        ;

    val {a} => bar: Option.Option a -> Option.Option a -> bool;
    let bar = fn a -> fn b ->
        match (a,b) with
            (Some 10, Some _) -> true,
            (None, None) -> false,
            _ -> false,
        ;
