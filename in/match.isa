module Option

    type Option a = None | Some a;

module Main
    
    val foo: Option.Option () -> int;
    let foo a =
        match a with
            Option.Some () -> 10,
            Option.None -> 10,
        ;

    val bar a: Option.Option a -> Option.Option a -> bool;
    let bar = fn a -> fn b ->
        match (a,b) with
            (Option.Some 10, Option.Some _) -> true,
            (Option.None, Option.None) -> false,
            _ -> false,
        ;
