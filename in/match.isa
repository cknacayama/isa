module option

    type Opt a = None | Some a;

module main with {option::Opt};
    
    val foo: Opt () -> int;
    let foo a =
        match a with
            Opt::Some () -> 10,
            Opt::None -> 10,
        ;

    val {a} => bar: Opt a -> Opt a -> bool;
    let bar = fn a -> fn b ->
        match (a,b) with
            (Opt::Some _, Opt::Some _) -> true,
            (Opt::None, Opt::None) -> false,
            _ -> false,
        ;
